{
    "name": "wsssoftware/cakephp-web-socket",
    "description": "WebSocket plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "WebSocket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebSocket\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
